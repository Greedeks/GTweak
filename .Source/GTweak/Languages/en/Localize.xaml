    <v:String x:Key="title_defrag_off_more">Disable defragmentation</v:String>
    <v:String x:Key="text_defrag_off_more">Disables defragmentation, extending the life of your SSD, saving it from unnecessary writes.</v:String>
    <v:String x:Key="title_defrag_on_more">Enable defragmentation</v:String>
