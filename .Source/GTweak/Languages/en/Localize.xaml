    <!--#region Applications Page -->
    <v:String x:Key="text0_applications">Applications are deleted</v:String>
    <v:String x:Key="text1_applications">without the possibility</v:String>
    <v:String x:Key="text2_applications">of recovery except OneDrive. Click on the</v:String>
