<ResourceDictionary xmlns="http://schemas.microsoft.com/winfx/2006/xaml/presentation"
                    xmlns:x="http://schemas.microsoft.com/winfx/2006/xaml"
                    xmlns:vm="clr-namespace:GTweak.Core.ViewModel"
                    xmlns:view="clr-namespace:GTweak.View">

    <DataTemplate DataType="{x:Type vm:ConfidentialityVM}">
        <view:ConfidentialityView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InterfaceVM}">
        <view:InterfaceView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ApplicationsVM}">
        <view:ApplicationsView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:ServicesVM}">
        <view:ServicesView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:SystemVM}">
        <view:SystemView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:InformationSystemVM}">
        <view:InformationSystemView />
    </DataTemplate>

    <DataTemplate DataType="{x:Type vm:MoreVM}">
        <view:MoreView />
    </DataTemplate>


</ResourceDictionary>